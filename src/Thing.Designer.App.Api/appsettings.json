{
  "Apps": {
    "BlazorClient": { "Origin": "https://localhost:8126" }
  },
  "ConnectionStrings": {
    "SqliteConnection": "Data Source=database.sqlite"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "ThingBuilder.Blazor.Client": {
        "Profile": "SPA",
        "RedirectUri": "https://localhost:8126/authentication/login-callback",
        "LogoutUri": "https://localhost:8126/authentication/logged-out"
      }
    }
  },
  "AllowedHosts": "*"
}
